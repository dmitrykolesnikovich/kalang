int hello() {
    return 6;
}

int memberInvoke() {
    return hello();
}

int staticInvoke() {
    return this.hello();
}

int dynamicInvoke(){
    return ((Integer)this->hello()).intValue();
}

int iteratingInvoke(){
    var ints = (<Integer>[4,5,6])*.intValue();
    return ints[2];
}

int iteratingDynamicInvoke() {
    var ints = (<Integer>[4,5,6])*->intValue();
    return (Integer)ints[2];
}

int safeInvoke(){
    val s as String? = null;
    if ( s?.length() == null) {
        return 6;
    }
    return 1;
}

int saveDynamicInvoke() {
    val s as Object? = null;
    if ( s?->length() == null) {
        return 6;
    }
    return 1;
}