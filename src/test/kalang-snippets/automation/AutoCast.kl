boolean bytePlusByte() {
    byte b1 = 127;
    byte b2 = 10;
    return b1 + b2 == 137;
}

boolean shortPlusShort() {
    short s1 = 32767;
    short s2 = 100;
    return s1 + s2 == 32867;
}

boolean integer2int() {
    val i as Object = 123;
    var j = 456;
    j = (Integer) i;
    return j == 123;
}